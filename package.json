{
  "name": "oor-webstudio",
  "version": "1.0.0",
  "description": "Out of Records Web Studio",
  "scripts": {
    "build": "node scripts/update-cache-version.js",
    "process-artists": "node scripts/process-artists.js",
    "migrate-paths": "node scripts/migrate-paths-to-wp.js",
    "migrate-paths:dry-run": "node scripts/migrate-paths-to-wp.js --dry-run",
    "setup": "bash scripts/setup-dev.sh",
    "serve": "python3 -m http.server 8040",
    "serve:alt": "npx serve . -p 8040"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "sharp": "^0.33.0",
    "fluent-ffmpeg": "^2.1.2"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.21"
  }
}

